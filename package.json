  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node ./server/server",
    "ng": "ng",
    "build": "ng build --prod",
    "postinstall": "ng build --aot --prod",
    "test": "echo \"Error: no test specified\"",
    "test-dev": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "client": "ng serve",
    "client-prod": "ng serve --prod",
    "client-aot": "ng serve -aot",
    "server-local":
      "NODE_ENV=local nodemon ./server/server --ignore ./server/judgingengine/temp",
    "server-local-debug":
      "NODE_ENV=local nodemon DEBUG=* ./server/server --ignore ./server/judgingengine/temp",
    "server-dev":
      "NODE_ENV=dev nodemon ./server/server --ignore ./server/judgingengine/temp",
    "server-stage":
      "NODE_ENV=stage nodemon ./server/server --ignore ./server/judgingengine/temp",
    "local": "concurrently \"npm run client\" \"npm run server-local \"",
    "local-debug":
      "concurrently \"npm run client\" \"npm run server-local-debug \"",
    "dev": "concurrently \"npm run client\" \"npm run server-dev \"",
    "stage": "concurrently \"npm run client\" \"npm run server-stage \"",
    "prod": "npm run client-prod"
  },
  "private": true,
